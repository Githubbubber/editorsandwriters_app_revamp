<!-- Log in section -->
<%= form_for :session do |f|%>
	<%= f.label :alter_ego %> BigBertha
	<%= f.text_field :alter_ego, maxlength: 20, required: "required" %>

	<%= f.label :password %>
	<%= f.password_field :password, minlength: 8, maxlength: 30, required: "required" %>

	<%= f.label :password_confirmation %>
	<%= f.password_field :password_confirmation, minlength: 8, maxlength: 30, required: "required" %>

	<%= f.submit "Log in" %>
<% end %>